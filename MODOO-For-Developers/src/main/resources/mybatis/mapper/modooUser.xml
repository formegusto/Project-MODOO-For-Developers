<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTDMapper3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
	<mapper namespace="com.wrk.mfd.repository.MuserRepository">
		<select id="authMuser" resultType="muser">
			SELECT id
			FROM USERS
			WHERE ID=#{id}
			AND PASSWORD=#{password}
		</select>
	</mapper>